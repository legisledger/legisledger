{
  "buildCommand": "echo 'No build needed'",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    },
    {
      "source": "/prototype",
      "destination": "/ui-prototypes/confidence-slider-prototype.html"
    },
    {
      "source": "/ui/:path*",
      "destination": "/ui-prototypes/:path*"
    },
    {
      "source": "/",
      "destination": "/ui-prototypes/confidence-slider-prototype.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
