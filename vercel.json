{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "ui-prototypes/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/prototype",
      "dest": "/ui-prototypes/confidence-slider-prototype.html"
    },
    {
      "src": "/ui/(.*)",
      "dest": "/ui-prototypes/$1"
    },
    {
      "src": "/",
      "dest": "/ui-prototypes/confidence-slider-prototype.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}